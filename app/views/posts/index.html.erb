<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <% @post.each do |post| %>
      <div class="well">
        <h2><%= post.title %></h2>
          <%= link_to "View Post", post_path(post), class: "btn btn-default" %>
      </div>
    <% end %>
      
    <%= link_to "New Post", new_post_path %>
  </div>
</div>
